{
    "version": 1,
    "formatters": {
        "default": {
            "format": "%(asctime)s | %(levelname)s | %(name)s | %(funcName)s | %(message)s"
        }
    },
    "handlers": {
        "serialfile": {
            "class": "logging.FileHandler",
            "formatter": "default",
            "filename": "%LOGFILE%"
        }
    },
    "loggers": {
        "fipy": {
            "level": "DEBUG",
            "handlers": ["serialfile"]
        }
    }
}
